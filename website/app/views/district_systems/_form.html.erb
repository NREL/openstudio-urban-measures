<%= bootstrap_form_tag url: (@district_system.new_record? ? '/district_systems' : "/district_systems/#{@district_system.id}"),  method: (@district_system.new_record? ? :post : :put), layout: :horizontal do |f| %>

  <fieldset>
    <%= f.file_field :geojson_file, control_col: 'col-sm-4' %>
  </fieldset>

  <%= f.form_group do %>
    <%= f.primary @district_system.new_record? ? 'Create' : 'Update' %>
  <% end %>
<% end %>

