{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://json-schema.org/openstudio-urban-modeling/region_properties.json#",
	"title": "Region object",
	"description": "Schema for a Region object",
	"type": "object",
	"properties": {
		"id": {
			"FIXME": "may be there, type not known (string? number?)",
			"description": "Unique id used to refer to this feature within this dataset, generated by database.",
			"type": "string"
		},
		"created_at": {
			"description": "Generated by database, YYYY-MM-DDTHH:MM:SS.SSSZ",
			"type": "string"
		},
		"updated_at": {
			"description": "Generated by database, YYYY-MM-DDTHH:MM:SS.SSSZ",
			"type": "string"
		},
		"parent_region_id": {
			"description": "Id of larger region that contains this one, managed by the database.",
			"type": "string"
		},
		"geojson_id": {
			"description": "Id of the geojson for this feature, managed by the database.",
			"type": "string"
		},
		"type": {
			"description": "Type of feature.",
			"type": "string",
			"enum": [
				"Building",
				"Taxlot",
				"Region",
				"District System"
			]
		},
		"source_name": {
			"description": "Name of the original data source.",
			"type": "string"
		},
		"source_id": {
			"description": "Id of the feature in original data source.",
			"type": "string"
		},
		"country_code": {
			"description": "3 digit ISO country code, e.g. 'USA'.",
			"type": "string"
		},
		"country_name": {
			"description": "Country name.",
			"type": "string"
		},
		"state_code": {
			"description": "Two character state code",
			"type": "string"
		},
		"state_name": {
			"description": "State name",
			"type": "string"
		},
		"city_name": {
			"description": "City name",
			"type": "string"
		},
		"fips_code": {
			"description": "Fice digit FIPS code.",
			"type": "string"
		},
		"county_name": {
			"description": "County name",
			"type": "string"
		},
		"census_tract": {
			"description": "Four to six digit census tract id.",
			"type": "string"
		},
		"census_block_group": {
			"description": "Census block group within the census tract.",
			"type": "string"
		},
		"census_block": {
			"description": "Census block within the census block group.",
			"type": "string"
		},
		"zip_code": {
			"description": "Five digit zip code.",
			"type": "string"
		},
		"footprint_area": {
			"description": "Area of the footprint (m^2)",
			"type": "number"
		},
		"footprint_area_source": {
			"$ref": "file:///E:/openstudio-urban-measures/building_properties.json#/definitions/source"
		},
		"footprint_perimeter": {
			"description": "Perimeter of the footprint (m)",
			"type": "number"
		},
		"footprint_perimeter_source": {
			"$ref": "file:///E:/openstudio-urban-measures/building_properties.json#/definitions/source"
		}
	},
	"required": [
		"type",
		"source_name",
		"source_id"
	],
	"definitions": {
		"source": {
			"description": "Data source",
			"type": "string",
			"enum": [
				"Measured",
				"Assessor",
				"Developer",
				"User Input",
				"Inferred"
			]
		}
	}
}