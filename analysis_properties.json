{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://json-schema.org/geojson/geojson.json#",
	"title": "Geo JSON object",
	"description": "Schema for a Geo JSON object",
	"type": "object",
	"properties": {
		"id": {
			"FIXME": "may be there, type not known (string? number?)",
			"description": "Unique id used to refer to this feature within this dataset, generated by database.",
			"type": "string"
		},
		"created_at": {
			"description": "Generated by database, YYYY-MM-DDTHH:MM:SS.SSSZ",
			"type": "string"
		},
		"updated_at": {
			"description": "Generated by database, YYYY-MM-DDTHH:MM:SS.SSSZ",
			"type": "string"
		},
		"source_name": {
			"description": "Name of the original data source.",
			"type": "string"
		},
		"source_id": {
			"description": "Id of the feature in original data source.",
			"type": "string"
		},
		"is_template_analysis": {
			"description": "True if this analysis is a tempalte which is not designed to run directly.",
			"type": "boolean"
		},
		"template_analysis_id": {
			"description": "Points to the template analysis this analysis was creted from.",
			"type": "string"
		},
		"dencity_id": {
			"description": "Id of corresponding analysis in DEnCity database.",
			"type": "string"
		},
		"dencity_url": {
			"description": "Url of DEnCity database.",
			"type": "string"
		},
		"analysis_type": {
			"type": "string",
			"enum": [
				"Manual",
				"Sensitivity",
				"Optimization",
				"Calibration",
				"Sampling"
			]
		},
		"input_type": {
			"type": "string",
			"enum": [
				"Building",
				"District System"
			]
		},
		"submitted_at": {
			"description": "YYYY-MM-DDTHH:MM:SS.SSSZ",
			"type": "string"
		},
		"completed_at": {
			"description": "YYYY-MM-DDTHH:MM:SS.SSSZ",
			"type": "string"
		},
		"variables": {
			"description": "Variables exposed in this analysis.",
			"items": {
				"$ref": "#/definitions/variable"
			}
		},
		"optimal_datapoint_id": {
			"description": "Optimal datapoint id, used when calibrating models.",
			"type": "string"
		}
	},
	"definitions": {
		"variable": {
			"properties": {
				"name": {
					"type": "string"
				},
				"display_name": {
					"type": "string"
				}
			},
			"additionalProperties": false
		},
		"source": {
			"description": "Data source",
			"type": "string",
			"enum": [
				"Measured",
				"Assessor",
				"Developer",
				"User Input",
				"Inferred"
			]
		}
	}
}